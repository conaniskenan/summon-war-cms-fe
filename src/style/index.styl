
.content
    margin 20px
    padding 20px
    height calc(100vh - 160px)
    background #ffffff
    border-radius 10px
.logo
    width 45px
    height 45px
    cursor pointer
.logo_title
    color #409eff
    margin-left 10px
    font-size 18px
.header
    display flex
    align-items center
    background-color #ffffff
    box-shadow rgb(228 230 242) 0px 2px 6px 0px
    position sticky
    z-index 200
.avatar
    width 40px !important
    height 40px !important
    margin-right 10px
    border-radius 50px
    margin-top 10px